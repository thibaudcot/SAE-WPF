<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="image\menu.jpg" />
    <None Remove="menu.jpg" />
    <None Remove="Police\Browood-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="image\menu.jpg" />
    <EmbeddedResource Include="Police\Browood-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="menu.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
